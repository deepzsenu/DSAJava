import java.util.*;
public class Main {
	static boolean pal(int n){
		int m = n;
		int r = 0;
		while(n>0){
			r = r*10+(n%10);
			n = n/10;
		}
		return m==r;
	}
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(pal(n));

	}
}
