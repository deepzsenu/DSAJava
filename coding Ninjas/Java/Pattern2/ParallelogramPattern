import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int s = n;
		for(int i = 0; i<n; i++){
			for(int j = 0; j<s; j++){
				if(j>=i){
					System.out.print("*");
				}
				else{
					System.out.print(" ");
				}
			}
			s++;
			System.out.println();

		}

	}
}
