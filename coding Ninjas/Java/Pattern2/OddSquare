import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		for(int i =1; i<=2*n; i+=2){
			int s = i;
			for (int j = 1; j <2*n; j++) {
				if(j%2!=0){
					System.out.print(s);
					s+=2;
				}
				if(s>2*n-1){
					s = 1;
				}				
			}
			System.out.println();
		}

	}
}
