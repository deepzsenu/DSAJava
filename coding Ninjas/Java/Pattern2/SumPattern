import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		for (int i  =1; i<=n;i++){
			int s = 1;
			for(int j = 0; j<i-1; j++){
				System.out.print(j+1+"+");
				s++;
			}
			long sum = (s*(s+1))/2;
			System.out.print(s+"="+sum);
			System.out.println();
		}
	}
}
