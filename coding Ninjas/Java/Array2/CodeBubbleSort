
public class Solution {  
    static void swap(int arr[], int i, int j ){
        int temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
    }

    public static void bubbleSort(int[] arr){
    	//Your code goes here
        for(int i = 0; i <arr.length-1; i++ ){
            for(int j = 0; j<arr.length-1; j++){
                if(arr[j]>arr[j+1]){
                    swap(arr, j , j+1);
                }
            }
        }
    }  

}